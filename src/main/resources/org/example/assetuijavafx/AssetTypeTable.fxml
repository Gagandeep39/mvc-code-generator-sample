<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.assetuijavafx.controllers.AssetTypeTableController">
    <TableView fx:id="table" prefWidth="720.0">
        <columns>
            <TableColumn fx:id="name" prefWidth="75.0" text="name"/>
            <TableColumn fx:id="expectedLifeSpan" prefWidth="75.0" text="expectedLifeSpan"/>
            <TableColumn fx:id="image" prefWidth="75.0" text="image"/>
            <TableColumn fx:id="update" prefWidth="75.0" text="Update"/>
            <TableColumn fx:id="delete" prefWidth="75.0" text="Delete"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
