<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.assetuijavafx.controllers.AssetTypeTableController">
    <TableView fx:id="table" prefWidth="720.0">
        <columns>
            <TableColumn fx:id="columnName" prefWidth="75.0" text="name"/>
            <TableColumn fx:id="columnExpectedLifeSpan" prefWidth="75.0" text="expectedLifeSpan"/>
            <TableColumn fx:id="columnImage" prefWidth="75.0" text="image"/>
            <TableColumn fx:id="columnUpdate" prefWidth="75.0" text="Update" style="-fx-alignment: center"/>
            <TableColumn fx:id="columnDelete" prefWidth="75.0" text="Delete" style="-fx-alignment: center"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
