<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml"
	fx:controller="org.example.assetuijavafx.fxml.controllers.AssetTypeTableController"
	fx:id="parentContainer"	
	spacing="16">
    <TableView fx:id="table">
        <columns>
			<TableColumn fx:id="columnName" prefWidth="75.0" text="name"/>
			<TableColumn fx:id="columnExpectedLifeSpan" prefWidth="75.0" text="expectedLifeSpan"/>
			<TableColumn fx:id="columnVisible" prefWidth="75.0" text="visible"/>
			<TableColumn fx:id="columnImage" prefWidth="75.0" text="image"/>
            <TableColumn fx:id="columnSpecificAssets" prefWidth="75.0" text="specificAssets" style="-fx-alignment: center"/>
            <TableColumn fx:id="columnUpdate" prefWidth="75.0" text="Update" style="-fx-alignment: center"/>
            <TableColumn fx:id="columnDelete" prefWidth="75.0" text="Delete" style="-fx-alignment: center"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</VBox>
