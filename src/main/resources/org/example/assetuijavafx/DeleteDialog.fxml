<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.assetuijavafx.controllers.DeleteDialogController">
    <children>
        <VBox>
            <children>
            <Label contentDisplay="CENTER" text="Are  you sure you want to Delete?">
               <padding>
                  <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
               </padding>
            </Label>
                <HBox alignment="CENTER" spacing="16.0">
                    <children>
                        <Button fx:id="buttonDelete" mnemonicParsing="false" onAction="#delete" text="Delete" />
                        <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
                    </children>
                </HBox>
            </children>
         <padding>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
         </padding>
        </VBox>
    </children>
</AnchorPane>
