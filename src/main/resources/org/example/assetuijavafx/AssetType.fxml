      fx:id="parentContainer"
      spacing="16">
    <children>
        <BreadCrumbBar fx:id="breadCrumbBar" prefWidth="Infinity"/>
        <VBox fx:id="childContainer">
            <fx:include source="AssetTypeTable.fxml"/>
